	public static void TwoSumProblemTest1(Integer[] a, int v) {

		Set<Integer> n = new HashSet<Integer>(Arrays.asList(a));

		int diff = 0;

		for (int i = 0; i < a.length; i++) {

			diff = v - a[i];

			if (n.contains(diff)) {

				System.out.print(a[i]);

				System.out.print(",");

				System.out.println(diff);

				n.remove(a[i]);

			}

		}

	}
